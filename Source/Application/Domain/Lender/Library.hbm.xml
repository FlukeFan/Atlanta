<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

    <class  name="Atlanta.Application.Domain.Lender.Library, Atlanta.Application.Domain"
            table="Library">

        <id name="Id" column="Library_Id">
            <generator class="native"/>
        </id>

        <bag name="OwnedMedia" lazy="true" inverse="true" cascade="all-delete-orphan">
            <key column="Library_Id" />
            <one-to-many class="Atlanta.Application.Domain.Lender.Media, Atlanta.Application.Domain"/>
        </bag>

    </class>

</hibernate-mapping>


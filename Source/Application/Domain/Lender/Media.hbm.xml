<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

    <class  name="Atlanta.Application.Domain.Lender.Media, Atlanta.Application.Domain" table="Media">

        <id name="Id" column="Media_Id">
            <generator class="native"/>
        </id>

        <many-to-one    name="OwningLibrary"    column="Library_Id"     class="Atlanta.Application.Domain.Lender.Library, Atlanta.Application.Domain"/>
        <property       name="Type"             column="Type"/>
        <property       name="Name"             column="Name"/>
        <property       name="Description"      column="Description"/>

    </class>

</hibernate-mapping>


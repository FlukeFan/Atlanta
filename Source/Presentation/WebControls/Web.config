<configuration>

    <appSettings>
        <add key="WebScriptDirectory"       value="/atlanta/web/script/" />
    </appSettings>

    <system.web>

        <httpRuntime
            executionTimeout="180"
        />

        <compilation
                debug="true"
                batch="true"
                defaultLanguage="c#"
                >

            <compilers>

                <compiler
                        language="c#"
                        type="Microsoft.CSharp.CSharpCodeProvider, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
                        compilerOptions="/optimize-"
                        />

            </compilers>
        </compilation>

        <sessionState
            mode="StateServer"
            stateConnectionString="tcpip=localhost:42424"
            cookieless="true"
            timeout="600"
        />

    </system.web>

</configuration>


